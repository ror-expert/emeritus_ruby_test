<table class="table">
  <thead>
    <tr>
      <th scope="col"> Student Name </th>
      <th scope="col"> Action </th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do | student | %>
      <tr id="<%= dom_id student %>">
        <td> <%= student.full_name %> </td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>